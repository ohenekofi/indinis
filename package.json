{
  "name": "indinis",
  "version": "0.1.0",
  "description": "A storage library using LSM trees for storage and B-trees for indices with MVCC support",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "install": "node-gyp rebuild",
    "build": "tsc",
    "test": "jest",
    "prepare": "npm run build",
    "test:cpp:configure": "node-gyp configure",
    "test:cpp:build": "node-gyp build --target=run_cpp_tests",
    "test:cpp:run": "build\\Release\\run_cpp_tests.exe",
    "test:cpp": "npm run test:cpp:build && npm run test:cpp:run",
    "test:cpp:filter": "npm run test:cpp:build && build\\Release\\run_cpp_tests.exe --gtest_filter",
    "test:cpp:verbose": "npm run test:cpp:build && build\\Release\\run_cpp_tests.exe --gtest_verbose",
    "test:cpp:list": "npm run test:cpp:build && build\\Release\\run_cpp_tests.exe --gtest_list_tests"
  },
  "keywords": [
    "storage",
    "database",
    "nosql",
    "lsm",
    "btree",
    "mvcc"
  ],
  "author": "Ohene Kofi",
  "license": "MIT",
  "dependencies": {
    "bindings": "^1.5.0",
    "node-addon-api": "^7.0.0"
  },
  "devDependencies": {
    "@types/bindings": "^1.5.5",
    "@types/jest": "^29.5.10",
    "@types/node": "^20.10.0",
    "@types/rimraf": "^4.0.5",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.2"
  },
  "gypfile": true,
  "engines": {
    "node": ">=16.0.0"
  }
}
